<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {{#each content}}
    {{#ifNotEq this.frontmatter.params.noIndex true}}
      <url>
        <loc>https://www.dariomac.com{{../frontmatter.card.linkto}}</loc>
        <lastmod>{{default ../frontmatter.updated_date ../frontmatter.date}}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.5</priority>
      </url>
    {{/ifNotEq}}
  {{/each}}
</urlset>
